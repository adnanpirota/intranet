<%= form_for [@warehouse_document, @warehouse_detail], remote: true, html: { class: "form-horizontal", style: "display:inline;" }  do |f| %>
  <div class="modal-body">
    <ul class="errors"></ul>
    
    <div class="control-group">
      <%= f.label :item_id, class: "control-label" %>
      <div class="controls">
        <%= f.text_field :item_id %>
      </div>
    </div>
    
    <div class="control-group">
      <%= f.label :sasia, class: "control-label" %>
      <div class="controls">
        <%= f.text_field :sasia %>
      </div>
    </div>
    
    <div class="control-group">
      <%= f.label :cmimi, class: "control-label" %>
      <div class="controls">
        <%= f.text_field :cmimi %>
      </div>
    </div>
    
    
    
  </div>
  <div class="modal-footer">
    <%= f.submit "Shto artikullin",   class: "btn btn-primary" %>
    <%= link_to "Anulo", "#", class: "btn", data: {dismiss: "modal"} %>
  </div>
<% end %>
<% provide(:title, "Pushimet") %>
<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="pending_info">
        <%= render 'shared/pending_info' %>
      </section>
      
    </aside>
    <div class="col-md-8">
      <h3>Pushimet</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <h3>Kycuni, ju lutem!</h3>  
<% end %>
  
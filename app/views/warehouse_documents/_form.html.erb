<%= form_for @warehouse_document, url: {action: "pranim_malli"},  html: { class: "form-horizontal", style: "display:inline;" }  do |f| %>
  
  <div class="modal-body">
    <ul class="errors"></ul>
    
    <div class="control-group">
      <%= f.label :request_id, class: "control-label" %>
      <div class="controls">
        <%= f.text_field :request_id %>
      </div>
    </div>
    
    <div class="control-group">
      <%= f.label :supplier_id, class: "control-label" %>
      <div class="controls">
        <%= f.collection_select :supplier_id, Supplier.order(:id), :id, :emri, include_blank: true %>
      </div>
    </div>
    
    <%= hidden_field(:pranimdalje, 1) %>
    
    
    
           
    
  </div>
  <div class="modal-footer">
    <%= f.submit "Shto artikujt",   class: "btn btn-primary" %>
    <%= link_to "Anulo", "#", class: "btn", data: {dismiss: "modal"} %>
  </div>

<% end %>